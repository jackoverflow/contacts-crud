    setContacts([...contacts, response.data]);\n  };\n\n  const updateContactHandler = async (contact) => {\n    const response = await api.put(`/contacts/${contact.id}`, contact);\n    const { id, name, email } = response.data;\n    setContacts(\n      contacts.map((contact) => {\n        return contact.id === id ? { ...response.data } : contact;\n      })\n    );\n  };\n\n  const removeContactHandler = async (id) => {\n    await api.delete(`/contacts/${id}`);\n    const newContactList = contacts.filter((contact) => {\n      return contact.id !== id;\n    });\n\n    setContacts(newContactList);\n  };\n\n  useEffect(() => {\n    // const retriveContacts = JSON.parse(localStorage.getItem(LOCAL_STORAGE_KEY));\n    // if (retriveContacts) setContacts(retriveContacts);\n    const getAllCOntacts = async () => {\n      const allContacts = await retrieveContacts();\n      if (allContacts) setContacts(allContacts);\n    };\n\n    getAllCOntacts();\n  }, []);\n\n  useEffect(() => {\n    //localStorage.setItem(LOCAL_STORAGE_KEY, JSON.stringify(contacts));\n  }, [contacts]);\n\n  return (\n    <div className=\"ui container\">\n      <Router>\n        <Header />\n        <Switch>\n          <Route\n            path=\"/\"\n            exact\n            render={(props) => (\n              <ContactList\n                {...props}\n                contacts={contacts}\n                getContactId={removeContactHandler}\n              />\n            )}\n          />\n          <Route\n            path=\"/add\"\n            render={(props) => (\n              <AddContact {...props} addContactHandler={addContactHandler} />\n            )}\n          />\n\n          <Route\n            path=\"/edit\"\n            render={(props) => (\n              <EditContact\n                {...props}\n                updateContactHandler={updateContactHandler}\n              />\n            )}\n          />\n\n          <Route path=\"/contact/:id\" component={ContactDetail} />\n        </Switch>\n      </Router>\n    </div>\n  );\n}\n\nexport default App;\n","/home/leeyo/Desktop/React_Tutorial_Contact_Manager_App-react-crud-axios/src/components/Header.js",[],"/home/leeyo/Desktop/React_Tutorial_Contact_Manager_App-react-crud-axios/src/components/ContactList.js",[],"/home/leeyo/Desktop/React_Tutorial_Contact_Manager_App-react-crud-axios/src/components/ContactDetail.js",[],"/home/leeyo/Desktop/React_Tutorial_Contact_Manager_App-react-crud-axios/src/components/EditContact.js",[],"/home/leeyo/Desktop/React_Tutorial_Contact_Manager_App-react-crud-axios/src/components/AddContact.js",[],"/home/leeyo/Desktop/React_Tutorial_Contact_Manager_App-react-crud-axios/src/api/contacts.js",[],"/home/leeyo/Desktop/React_Tutorial_Contact_Manager_App-react-crud-axios/src/components/ContactCard.js",[],["45","46"],{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","severity":1,"message":"52","line":36,"column":17,"nodeType":"53","messageId":"54","endLine":36,"endColumn":21},{"ruleId":"51","severity":1,"message":"55","line":36,"column":23,"nodeType":"53","messageId":"54","endLine":36,"endColumn":28},{"ruleId":"47","replacedBy":"56"},{"ruleId":"49","replacedBy":"57"},"no-native-reassign",["58"],"no-negated-in-lhs",["59"],"no-unused-vars","'name' is assigned a value but never used.","Identifier","unusedVar","'email' is assigned a value but never used.",["58"],["59"],"no-global-assign","no-unsafe-negation"]